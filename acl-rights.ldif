dn: olcDatabase={1}mdb,cn=config
changetype: modify
delete: olcAccess
-
add: olcAccess
# Root SASL EXTERNAL a tous les droits
olcAccess: {0}to * by dn.exact=gidNumber=0+uidNumber=0,cn=peercred,cn=external,cn=auth manage
# Groupe adminldap a tous les droits
olcAccess: {1}to * by group.exact="cn=adminldap,ou=admins,dc=myguest,dc=virtualbox,dc=org" manage
# Tout le monde peut lire
olcAccess: {2}to * by * read
